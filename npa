// Глобальные переменные
let selectedSources = new Set(['all']);
let favorites = JSON.parse(localStorage.getItem('documentFavorites')) || [];
let history = JSON.parse(localStorage.getItem('documentHistory')) || [];
let currentTourStep = 0;
let searchIndex = [];
let currentBreadcrumbsPath = [];

// Данные для тура обучения
const tourSteps = [
    {
        title: "Добро пожаловать!",
        content: "Это обучение поможет вам освоить основные функции карты документов. Вы узнаете, как эффективно работать с системой."
    },
