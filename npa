        content: "Сохраняйте важные документы в Избранное и просматривайте историю посещений через кнопки в правом верхнем углу."
    }
];

// Функция для генерации уникального идентификатора документа
function generateDocId(doc) {
    const str = `${doc.title}-${doc.date}`;
    let hash = 0;
    for (let i = 0; i < str.length; i++) {
        const char = str.charCodeAt(i);
        hash = ((hash << 5) - hash) + char;
        hash = hash & hash;
    }
    return 'doc_' + Math.abs(hash).toString(16);
}

// Функция для сортировки документов по источникам
function sortDocumentsBySource(documents) {
    return documents.sort((a, b) => {
        const aSources = Array.isArray(a.source) ? a.source : [a.source];
        const bSources = Array.isArray(b.source) ? b.source : [b.source];
        
        const orderA = sourceOrder.indexOf(aSources[0]);
        const orderB = sourceOrder.indexOf(bSources[0]);
        
        if (orderA !== orderB) {
            return orderA - orderB;
        }
        
        if (aSources.length > 1 && bSources.length > 1) {
            const orderA2 = sourceOrder.indexOf(aSources[1]);
            const orderB2 = sourceOrder.indexOf(bSources[1]);
            return orderA2 - orderB2;
        }
        
        return 0;
    });
}

// Функция для инициализации карта документов
function initDocumentMap() {
    const mapContainer = document.getElementById('documentMap');
    const sourcesList = document.getElementById('sourcesList');
    
    generateDocumentIds(documentMapData);
    generateSourcesList(sourcesList);
    generateDocumentTree(mapContainer, documentMapData);
    buildSearchIndex();
    setupEventHandlers();
    
    const savedTheme = localStorage.getItem('theme') || 'light';
    if (savedTheme === 'dark') {
        document.body.classList.add('dark-theme');
        document.querySelector('#themeToggle i').className = 'fas fa-sun';
    }
}

// Функция для генерации ID для всех документов
function generateDocumentIds(data) {
    function processNode(node) {
        if (node.documents) {
            node.documents.forEach(doc => {
                if (!doc.id) {
                    doc.id = generateDocId(doc);
                }
            });
        }
        if (node.children) {
            node.children.forEach(processNode);
        }
    }
    
    data.forEach(processNode);
}

// Функция для генерации поискового индекса
function buildSearchIndex() {
    searchIndex = [];
    
    function indexNode(node, path = []) {
        const nodePath = [...path, { id: node.id, title: node.title }];
        
        if (node.documents) {
            node.documents.forEach(doc => {
                searchIndex.push({
                    id: doc.id,
                    title: doc.title,
                    content: doc.content || '',
                    source: Array.isArray(doc.source) ? doc.source : [doc.source],
                    date: doc.date,
                    path: nodePath
                });
            });
        }
        
        if (node.children) {
            node.children.forEach(child => indexNode(child, nodePath));
        }
    }
    
    documentMapData.forEach(domain => indexNode(domain));
}

// Функция для поиска подсказок
function getSearchSuggestions(query) {
    if (!query || query.length < 2) return [];
    
    const lowerQuery = query.toLowerCase();
    return searchIndex
        .filter(item => 
            item.title.toLowerCase().includes(lowerQuery) ||
            item.content.toLowerCase().includes(lowerQuery)
        )
        .slice(0, 10);
}

// Функция для показа подсказок поиска
function showSearchSuggestions(suggestions) {
    const container = document.getElementById('searchSuggestions');
    container.innerHTML = '';
    
    if (suggestions.length === 0) {
        container.style.display = 'none';
        return;
    }
    
    suggestions.forEach(suggestion => {
        const div = document.createElement('div');
        div.className = 'search-suggestion';
        div.textContent = suggestion.title;
        div.onclick = () => {
            document.getElementById('searchInput').value = suggestion.title;
            container.style.display = 'none';
            filterDocuments(suggestion.title);
            navigateTo(suggestion.path[suggestion.path.length - 1].id);
        };
        container.appendChild(div);
    });
    
    container.style.display = 'block';
}

// Функция для генерации списка источников
function generateSourcesList(container) {
    const sources = {};
    
    function processDocuments(docs) {
        docs.forEach(doc => {
            const docSources = Array.isArray(doc.source) ? doc.source : [doc.source];
            docSources.forEach(source => {
                if (!sources[source]) {
                    sources[source] = 0;
                }
                sources[source]++;
            });
        });
    }
    
    function processNode(node) {
        if (node.documents) {
            processDocuments(node.documents);
        }
        if (node.children) {
            node.children.forEach(processNode);
        }
    }
    
    documentMapData.forEach(processNode);
    
    const sortedSources = sourceOrder.filter(source => sources[source] > 0);
    let totalCount = 0;
    Object.values(sources).forEach(count => totalCount += count);
    
    const allSourcesItem = document.createElement('div');
    allSourcesItem.className = 'source-item active';
    allSourcesItem.dataset.source = 'all';
    allSourcesItem.innerHTML = `
        <div class="source-icon" style="background-color: #ffffff; border: 1px solid #10b981"></div>
        <span class="source-name">Все источники</span>
        <span class="source-count">${totalCount}</span>
    `;
    container.appendChild(allSourcesItem);
    
    sortedSources.forEach(source => {
        const sourceItem = document.createElement('div');
        sourceItem.className = 'source-item';
        sourceItem.dataset.source = source;
        sourceItem.innerHTML = `
            <div class="source-icon" style="background-color: ${sourceColors[source] || '#6b7280'}"></div>
            <span class="source-name">${source}</span>
            <span class="source-count">${sources[source]}</span>
        `;
        container.appendChild(sourceItem);
    });
}

// Функция для генерации дерева документов
function generateDocumentTree(container, data) {
    container.innerHTML = '';
    data.forEach(domain => {
        const domainElement = createDomainElement(domain);
        container.appendChild(domainElement);
    });
}

// Функция для создания элемента домена
function createDomainElement(node, level = 0) {
    const element = document.createElement('div');
    let className = '';
    let titleContent = '';
    
    switch (level) {
        case 0:
            className = 'domain';
            titleContent = `
                <span>${node.title}</span>
                <span class="domain-code">${node.id}</span>
                <span class="expand-icon"></span>
            `;
            break;
        case 1:
            className = 'subdomain';
            titleContent = `
                <span>${node.title}</span>
                <span class="subdomain-code">${node.id}</span>
                <span class="expand-icon"></span>
            `;
            break;
        case 2:
            className = 'subsubdomain';
            titleContent = `
                <span>${node.title}</span>
                <span class="subsubdomain-code">${node.id}</span>
                <span class="expand-icon"></span>
            `;
            break;
    }
    
    element.className = className;
    element.dataset.id = node.id;
    
    const titleElement = document.createElement('div');
    titleElement.className = 'title';
    titleElement.innerHTML = titleContent;
    element.appendChild(titleElement);
    
    if (node.children || node.documents) {
        const childrenContainer = document.createElement('div');
        childrenContainer.className = 'children';
        
        if (node.children) {
            node.children.forEach(child => {
                const childElement = createDomainElement(child, level + 1);
                childrenContainer.appendChild(childElement);
            });
        }
        
        if (node.documents) {
            const sortedDocuments = sortDocumentsBySource(node.documents);
            const docList = document.createElement('ul');
            docList.className = 'doc-list';
            
            sortedDocuments.forEach(doc => {
                const docItem = createDocumentElement(doc);
                docList.appendChild(docItem);
            });
            
            childrenContainer.appendChild(docList);
        }
        
        element.appendChild(childrenContainer);
    } else {
        const emptyMessage = document.createElement('div');
        emptyMessage.className = 'empty-message';
        emptyMessage.textContent = 'Нет документов в этом разделе';
        element.appendChild(emptyMessage);
    }
    
    return element;
}

// Функция для создания элемента документа
function createDocumentElement(doc) {
    const li = document.createElement('li');
    li.className = 'doc-item';
    li.dataset.id = doc.id;
    
    const docSources = Array.isArray(doc.source) ? doc.source : [doc.source];
    li.dataset.sources = docSources.join(',');
    
    const iconsContainer = document.createElement('div');
    iconsContainer.className = 'document-icons';
    
    docSources.forEach(source => {
        const docIcon = document.createElement('span');
        docIcon.className = 'document-icon';
        docIcon.style.backgroundColor = sourceColors[source] || '#6b7280';
        iconsContainer.appendChild(docIcon);
    });
    
    const docTitle = document.createElement('span');
    docTitle.className = 'doc-title';
    docTitle.textContent = doc.title;
    
    li.appendChild(iconsContainer);
    li.appendChild(docTitle);
    
    return li;
}

// Функция для применения фильтра к элементу
function applyFilterToElement(element) {
    if (element.classList.contains('domain') || 
        element.classList.contains('subdomain') || 
        element.classList.contains('subsubdomain')) {
        const visibleDocs = element.querySelectorAll('.doc-item:not(.hidden)').length;
        const visibleChildren = element.querySelectorAll('.domain:not(.hidden), .subdomain:not(.hidden), .subsubdomain:not(.hidden)').length;
        
        if (visibleDocs > 0 || visibleChildren > 0) {
            element.classList.remove('hidden');
        } else {
            element.classList.add('hidden');
        }
    }
}

// Функция для применения фильтра к документу
function applyFilterToDocument(docElement) {
    const sources = docElement.dataset.sources.split(',');
    
    if (selectedSources.has('all') || sources.some(source => selectedSources.has(source))) {
        docElement.classList.remove('hidden');
    } else {
        docElement.classList.add('hidden');
    }
}

// Функция для раскрытия доменов с документами выбранного источника
function expandDomainsWithSourceDocuments() {
    if (selectedSources.has('all')) {
        document.querySelectorAll('.domain, .subdomain, .subsubdomain').forEach(item => {
            item.classList.remove('expanded');
        });
        return;
    }
    
    document.querySelectorAll('.domain, .subdomain, .subsubdomain').forEach(item => {
        item.classList.remove('expanded');
    });
    
    const sourceDocuments = document.querySelectorAll('.doc-item:not(.hidden)');
    if (sourceDocuments.length === 0) return;
    
    sourceDocuments.forEach(doc => {
        let parent = doc.closest('.subsubdomain, .subdomain, .domain');
        while (parent) {
            parent.classList.add('expanded');
            parent = parent.parentElement.closest('.subsubdomain, .subdomain, .domain');
        }
    });
}

// Функция для обновления хлебных крошек
function updateBreadcrumbs(path = []) {
    currentBreadcrumbsPath = path;
    const breadcrumbs = document.getElementById('breadcrumbs');
    const list = breadcrumbs.querySelector('.breadcrumbs-list');
    list.innerHTML = '';
    
    if (path.length === 0) {
        return;
    }
    
    path.forEach((item, index) => {
        const isLast = index === path.length - 1;
        
        const breadcrumb = document.createElement('span');
        breadcrumb.className = `breadcrumb-item ${isLast ? 'active' : ''}`;
        breadcrumb.textContent = item.title;
        breadcrumb.onclick = isLast ? null : () => navigateTo(item.id);
        
        list.appendChild(breadcrumb);
        
        if (!isLast) {
            const separator = document.createElement('span');
            separator.className = 'breadcrumb-separator';
            separator.textContent = '›';
            list.appendChild(separator);
        }
    });
}

// Функция для навигации
function navigateTo(nodeId) {
    const element = document.querySelector(`[data-id="${nodeId}"]`);
    if (element) {
        let parent = element.parentElement.closest('.domain, .subdomain, .subsubdomain');
        while (parent) {
            parent.classList.add('expanded');
            parent = parent.parentElement.closest('.domain, .subdomain, .subsubdomain');
        }
        
        element.scrollIntoView({ behavior: 'smooth', block: 'center' });
        updateBreadcrumbsForNode(nodeId);
    }
}

// Функция для получения пути к узлу
function updateBreadcrumbsForNode(nodeId) {
    let path = [];
    
    function findPath(node, currentPath = []) {
        const newPath = [...currentPath, { id: node.id, title: node.title }];
        
        if (node.id === nodeId) {
            path = newPath;
            return true;
        }
        
        if (node.children) {
            for (const child of node.children) {
                if (findPath(child, newPath)) return true;
            }
        }
        
        return false;
    }
    
    for (const domain of documentMapData) {
        if (findPath(domain, [])) break;
    }
    
    updateBreadcrumbs(path);
}

// Функция для сброса навигации
function resetNavigation() {
    document.querySelectorAll('.domain, .subdomain, .subsubdomain').forEach(item => {
        item.classList.remove('expanded');
    });
    updateBreadcrumbs([]);
    filterDocuments('');
}

// Функция для добавления в избранное
function toggleFavorite(docId) {
    const index = favorites.indexOf(docId);
    if (index === -1) {
        favorites.push(docId);
    } else {
        favorites.splice(index, 1);
    }
    localStorage.setItem('documentFavorites', JSON.stringify(favorites));
    updateFavoriteButton(docId);
}

// Функция для обновления кнопки избранного
function updateFavoriteButton(docId) {
    const button = document.getElementById('addToFavorites');
    if (button) {
        const isFavorite = favorites.includes(docId);
        button.innerHTML = isFavorite ? 
            '<i class="fas fa-star"></i>' : 
            '<i class="far fa-star"></i>';
        button.title = isFavorite ? 'Удалить из избранного' : 'Добавить в избранное';
    }
}

// Функция для добавления в историю
function addToHistory(doc) {
    const historyItem = {
        id: doc.id,
        title: doc.title,
        timestamp: new Date().toISOString(),
        source: Array.isArray(doc.source) ? doc.source[0] : doc.source
    };
    
    history = history.filter(item => item.id !== doc.id);
    history.unshift(historyItem);
    history = history.slice(0, 50);
    
    localStorage.setItem('documentHistory', JSON.stringify(history));
}

// Функция для показа избранного
function showFavorites() {
    const modal = document.getElementById('favoritesModal');
    const list = document.getElementById('favoritesList');
    list.innerHTML = '';
    
    if (favorites.length === 0) {
        list.innerHTML = '<p>Нет избранных документов</p>';
        modal.style.display = 'block';
        return;
    }
    
    favorites.forEach(docId => {
        const doc = findDocumentById(docId);
        if (doc) {
            const item = document.createElement('div');
            item.className = 'favorite-item';
            item.innerHTML = `
                <h4>${doc.title}</h4>
                <p>${formatDate(doc.date)} • ${Array.isArray(doc.source) ? doc.source[0] : doc.source}</p>
            `;
            item.onclick = () => {
                modal.style.display = 'none';
                showDocumentModal(doc);
            };
            list.appendChild(item);
        }
    });
    
    modal.style.display = 'block';
}

// Функция для показа истории
function showHistory() {
    const modal = document.getElementById('historyModal');
    const list = document.getElementById('historyList');
    list.innerHTML = '';
    
    if (history.length === 0) {
        list.innerHTML = '<p>История просмотров пуста</p>';
        modal.style.display = 'block';
        return;
    }
    
    history.forEach(item => {
        const doc = findDocumentById(item.id);
        if (doc) {
            const historyItem = document.createElement('div');
            historyItem.className = 'history-item';
            historyItem.innerHTML = `
                <h4>${doc.title}</h4>
                <p>${formatDate(item.timestamp)} • ${item.source}</p>
            `;
            historyItem.onclick = () => {
                modal.style.display = 'none';
                showDocumentModal(doc);
            };
            list.appendChild(historyItem);
        }
    });
    
    modal.style.display = 'block';
}

// Функция для запуска тура
function startTour() {
    currentTourStep = 0;
    const modal = document.getElementById('tourModal');
    const content = document.getElementById('tourContent');
    
    content.innerHTML = `
        <div class="tour-step active">
            <h3>${tourSteps[0].title}</h3>
            <p>${tourSteps[0].content}</p>
        </div>
    `;
    
    modal.style.display = 'block';
    updateTourNavigation();
}

// Функция для показа шага тура
function showTourStep(step) {
    const content = document.getElementById('tourContent');
    content.innerHTML = `
        <div class="tour-step active">
            <h3>${tourSteps[step].title}</h3>
            <p>${tourSteps[step].content}</p>
        </div>
    `;
    updateTourNavigation();
}

// Функция для обновления навигации тура
function updateTourNavigation() {
    document.getElementById('tourPrev').disabled = currentTourStep === 0;
    document.getElementById('tourNext').disabled = currentTourStep === tourSteps.length - 1;
}

// Функция для переключения темы
function toggleTheme() {
    const body = document.body;
    const isDark = body.classList.contains('dark-theme');
    const icon = document.querySelector('#themeToggle i');
    
    if (isDark) {
        body.classList.remove('dark-theme');
        body.classList.add('light-theme');
        icon.className = 'fas fa-moon';
    } else {
        body.classList.remove('light-theme');
        body.classList.add('dark-theme');
        icon.className = 'fas fa-sun';
    }
    
    localStorage.setItem('theme', isDark ? 'light' : 'dark');
}

// Функция для применения фильтра по источникам
function applySourceFilter() {
    document.querySelectorAll('.doc-item').forEach(applyFilterToDocument);
    document.querySelectorAll('.subsubdomain, .subdomain, .domain').forEach(applyFilterToElement);
    expandDomainsWithSourceDocuments();
}

// Функция для поиска документа по ID
function findDocumentById(id) {
    let result = null;
    
    function searchInNode(node) {
        if (node.documents) {
            const found = node.documents.find(doc => doc.id === id);
            if (found) {
                result = found;
                return true;
            }
        }
        
        if (node.children) {
            for (let child of node.children) {
                if (searchInNode(child)) {
                    return true;
                }
            }
        }
        
        return false;
    }
    
    for (let domain of documentMapData) {
        if (searchInNode(domain)) {
            break;
        }
    }
    
    return result;
}

// Функция для показа модального окна с информацией о документе
function showDocumentModal(doc) {
    const modal = document.getElementById('documentModal');
    const modalTitle = document.getElementById('modalTitle');
    const modalDates = document.getElementById('modalDates');
    const modalSource = document.getElementById('modalSource');
    const modalDomains = document.getElementById('modalDomains');
    const modalContent = document.getElementById('modalContent');
    const modalLink = document.getElementById('modalLink');
    
    modalTitle.textContent = doc.title;
    modalDates.innerHTML = '';
    
    if (doc.date) {
        const publishDate = document.createElement('div');
        publishDate.className = 'date-item';
        publishDate.innerHTML = `
            <i class="fas fa-calendar-plus"></i>
            <span>Дата публикации: ${formatDate(doc.date)}</span>
        `;
        modalDates.appendChild(publishDate);
    }
    
    if (doc.lastEditDate) {
        const editDate = document.createElement('div');
        editDate.className = 'date-item';
        editDate.innerHTML = `
            <i class="fas fa-edit"></i>
            <span>Последняя редактура: ${formatDate(doc.lastEditDate)}</span>
        `;
        modalDates.appendChild(editDate);
    }
    
    const docSources = Array.isArray(doc.source) ? doc.source : [doc.source];
    modalSource.innerHTML = '';
    
    docSources.forEach((source, index) => {
        const sourceElement = document.createElement('div');
        sourceElement.className = 'modal-source';
        sourceElement.innerHTML = `
            <i class="fas fa-file-alt"></i>
            <span>${source}</span>
        `;
        sourceElement.style.border = `1px solid ${sourceColors[source] || '#10b981'}`;
        sourceElement.style.backgroundColor = 'transparent';
        sourceElement.style.color = sourceColors[source] || '#64748b';
        sourceElement.style.marginBottom = index < docSources.length - 1 ? '5px' : '0';
        modalSource.appendChild(sourceElement);
    });
    
    const allDomains = findAllDomainsForDocument(doc.id);
    modalDomains.innerHTML = '';
    
    if (allDomains.length > 0) {
        allDomains.forEach(domainInfo => {
            const domainTag = document.createElement('span');
            domainTag.className = 'domain-tag';
            domainTag.textContent = `${domainInfo.code}: ${domainInfo.name}`;
            domainTag.title = domainInfo.code;
            modalDomains.appendChild(domainTag);
        });
    }
    
    modalContent.textContent = doc.content || 'Описание документа отсутствует';
    
    if (doc.link) {
        modalLink.innerHTML = `
            <a href="${doc.link}" target="_blank" rel="noopener">
                <i class="fas fa-external-link-alt"></i>
                <span>Перейти к первоисточнику</span>
            </a>
        `;
    } else {
        modalLink.innerHTML = '<span>Ссылка на первоисточник отсутствует</span>';
    }
    
    addToHistory(doc);
    updateFavoriteButton(doc.id);
    document.getElementById('addToFavorites').onclick = () => toggleFavorite(doc.id);
    modal.style.display = 'block';
}

// Функция для форматирования даты в читаемый вид
function formatDate(dateString) {
    if (!dateString) return 'Не указана';
    
    try {
        const date = new Date(dateString);
        return date.toLocaleDateString('ru-RU', {
            year: 'numeric',
            month: 'long',
            day: 'numeric'
        });
    } catch (e) {
        return dateString;
    }
}

// Функция для поиска ВСЕХ доменов, к которым относится документ
function findAllDomainsForDocument(docId) {
    const domains = [];
    
    function searchInNode(node, path = []) {
        let foundInThisNode = false;
        
        if (node.documents) {
            foundInThisNode = node.documents.some(doc => doc.id === docId);
            if (foundInThisNode) {
                const fullPath = [...path, node];
                fullPath.forEach(pathNode => {
                    domains.push({
                        code: pathNode.id,
                        name: domainCodeToName[pathNode.id] || pathNode.title
                    });
                });
            }
        }
        
        if (node.children) {
            const newPath = [...path, node];
            for (let child of node.children) {
                searchInNode(child, newPath);
            }
        }
    }
    
    for (let domain of documentMapData) {
        searchInNode(domain);
    }
    
    const uniqueDomains = [];
    const seen = new Set();
    
    domains.forEach(domain => {
        if (!seen.has(domain.code)) {
            seen.add(domain.code);
            uniqueDomains.push(domain);
        }
    });
    
    return uniqueDomains;
}

// Функция для сброса всех фильтров
function resetAllFilters() {
    // Сбрасываем поиск
    document.getElementById('searchInput').value = '';
    document.querySelector('.clear-search').classList.remove('visible');
    document.getElementById('searchSuggestions').style.display = 'none';
    
    // Сбрасываем источники
    selectedSources.clear();
    selectedSources.add('all');
    document.querySelectorAll('.source-item').forEach(i => {
        i.classList.remove('active');
        if (i.dataset.source === 'all') {
            i.classList.add('active');
        }
    });
    
    // Сбрасываем фильтрацию
    filterDocuments('');
    
    // Сворачиваем все домены
    document.querySelectorAll('.domain, .subdomain, .subsubdomain').forEach(item => {
        item.classList.remove('expanded');
    });
    
    // Сбрасываем хлебные крошки
    updateBreadcrumbs([]);
}

// Функция для фильтрации документов по поисковому запросу
function filterDocuments(searchTerm) {
    const docItems = document.querySelectorAll('.doc-item');
    const domainElements = document.querySelectorAll('.domain, .subdomain, .subsubdomain');
    let visibleCount = 0;
    
    docItems.forEach(item => {
        item.classList.add('hidden');
        item.innerHTML = item.innerHTML.replace(/<span class="highlight">(.+?)<\/span>/gi, '$1');
    });

    if (searchTerm) {
        const searchLower = searchTerm.toLowerCase();
        
        docItems.forEach(item => {
            const title = item.querySelector('.doc-title').textContent.toLowerCase();
            if (title.includes(searchLower)) {
                item.classList.remove('hidden');
                visibleCount++;
                
                const regex = new RegExp(`(${searchTerm})`, 'gi');
                const docTitle = item.querySelector('.doc-title');
                docTitle.innerHTML = docTitle.textContent.replace(regex, '<span class="highlight">$1</span>');
            }
        });
        
        domainElements.forEach(element => {
            const hasVisibleDocs = element.querySelectorAll('.doc-item:not(.hidden)').length > 0;
            const hasVisibleChildren = element.querySelectorAll('.domain:not(.hidden), .subdomain:not(.hidden), .subsubdomain:not(.hidden)').length > 0;
            
            if (!hasVisibleDocs && !hasVisibleChildren) {
                element.classList.add('hidden');
            } else {
                element.classList.remove('hidden');
                
                if (hasVisibleDocs) {
                    element.classList.add('expanded');
                }
            }
        });
    } else {
        docItems.forEach(item => {
            item.classList.remove('hidden');
        });
        
        domainElements.forEach(element => {
            element.classList.remove('hidden');
        });
        
        applySourceFilter();
    }
    
    const noResults = document.getElementById('noResults');
    if (visibleCount === 0 && searchTerm) {
        if (!noResults) {
            const noResultsElement = document.createElement('div');
            noResultsElement.id = 'noResults';
            noResultsElement.className = 'no-results';
            noResultsElement.textContent = 'Документы по вашему запросу не найдены';
            document.getElementById('documentMap').appendChild(noResultsElement);
        }
    } else if (noResults) {
        noResults.remove();
    }
}

// Функция для настройки обработчиков событий
function setupEventHandlers() 
{
    document.querySelectorAll('.domain > .title, .subdomain > .title, .subsubdomain > .title').forEach(title => {
        title.addEventListener('click', function() {
            const parent = this.parentElement;
            parent.classList.toggle('expanded');
            if (parent.classList.contains('expanded')) {
                updateBreadcrumbsForNode(parent.dataset.id);
            }
        });
    });
    
    const searchInput = document.getElementById('searchInput');
    const clearSearchBtn = document.querySelector('.clear-search');
    
    searchInput.addEventListener('input', function(e) {
        const searchTerm = e.target.value.toLowerCase().trim();
        const suggestions = getSearchSuggestions(searchTerm);
        showSearchSuggestions(suggestions);
        
        if (searchTerm) {
            clearSearchBtn.classList.add('visible');
        } else {
            clearSearchBtn.classList.remove('visible');
        }
        
        filterDocuments(searchTerm);
    });
    
    clearSearchBtn.addEventListener('click', function() {
        searchInput.value = '';
        clearSearchBtn.classList.remove('visible');
        document.getElementById('searchSuggestions').style.display = 'none';
        filterDocuments('');
    });
    
    document.getElementById('documentMap').addEventListener('click', function(e) {
        const docItem = e.target.closest('.doc-item');
        if (docItem) {
            const docId = docItem.dataset.id;
            const doc = findDocumentById(docId);
            if (doc) {
                showDocumentModal(doc);
            }
        }
    });
    
    document.getElementById('expandAllBtn').addEventListener('click', function() {
        document.querySelectorAll('.domain, .subdomain, .subsubdomain').forEach(item => {
            item.classList.add('expanded');
        });
    });
    
    document.getElementById('collapseAllBtn').addEventListener('click', function() {
        document.querySelectorAll('.domain, .subdomain, .subsubdomain').forEach(item => {
            item.classList.remove('expanded');
        });
    });
    
    document.querySelectorAll('.source-item').forEach(item => {
        item.addEventListener('click', function() {
            const source = this.dataset.source;
            
            if (source === 'all') {
                if (!selectedSources.has('all')) {
                    selectedSources.clear();
                    selectedSources.add('all');
                    document.querySelectorAll('.source-item').forEach(i => i.classList.remove('active'));
                    this.classList.add('active');
                }
            } else {
                selectedSources.delete('all');
                document.querySelectorAll('.source-item[data-source="all"]').forEach(i => i.classList.remove('active'));
                
                if (selectedSources.has(source)) {
                    selectedSources.delete(source);
                    this.classList.remove('active');
                } else {
                    selectedSources.add(source);
                    this.classList.add('active');
                }
                
                if (selectedSources.size === 0) {
                    selectedSources.add('all');
                    document.querySelectorAll('.source-item[data-source="all"]').forEach(i => i.classList.add('active'));
                }
            }
            
            applySourceFilter();
        });
    });
    
    // Исправленный обработчик для кнопки "Сбросить фильтры"
    document.getElementById('resetFilters').addEventListener('click', resetAllFilters);
    
    const modal = document.getElementById('documentModal');
    const closeButton = document.querySelector('.close');
    
    closeButton.addEventListener('click', function(e) {
        e.stopPropagation();
        modal.style.display = 'none';
    });
    
    modal.addEventListener('click', function(e) {
        if (e.target === modal) {
            modal.style.display = 'none';
        }
    });
    
    document.addEventListener('keydown', function(e) {
        if (e.key === 'Escape' && modal.style.display === 'block') {
            modal.style.display = 'none';
        }
    });
    
    document.getElementById('backToTop').addEventListener('click', function() {
        window.scrollTo({ top: 0, behavior: 'smooth' });
    });
    
    window.addEventListener('scroll', function() {
        const backButton = document.getElementById('backToTop');
        if (window.scrollY > 300) {
            backButton.style.display = 'flex';
        } else {
            backButton.style.display = 'none';
        }
    });
    
    document.getElementById('themeToggle').addEventListener('click', toggleTheme);
    document.getElementById('favoritesBtn').addEventListener('click', showFavorites);
    document.getElementById('historyBtn').addEventListener('click', showHistory);
    document.getElementById('tourBtn').addEventListener('click', startTour);
    
    document.getElementById('tourPrev').addEventListener('click', () => {
        if (currentTourStep > 0) {
            currentTourStep--;
            showTourStep(currentTourStep);
        }
    });
    
    document.getElementById('tourNext').addEventListener('click', () => {
        if (currentTourStep < tourSteps.length - 1) {
            currentTourStep++;
            showTourStep(currentTourStep);
        }
    });
    
    document.getElementById('tourComplete').addEventListener('click', () => {
        document.getElementById('tourModal').style.display = 'none';
    });
    
    document.querySelectorAll('#favoritesModal .close, #historyModal .close, #tourModal .close').forEach(closeBtn => {
        closeBtn.addEventListener('click', function() {
            this.closest('.modal').style.display = 'none';
        });
    });
}

// Инициализация при загрузке страницы
document.addEventListener('DOMContentLoaded', function() {
    initDocumentMap();
});
    </script>
</body>
</html>
